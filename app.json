{
  "expo": {
    "name": "niloticus-carev2",
    "slug": "niloticus-carev2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/niloticus-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.thawne23.niloticuscarev2"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-sqlite"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "9753a2dc-4a60-4869-9f2d-3a8883fc0049"
      }
    },
    "owner": "thawne-23"
  }
}
